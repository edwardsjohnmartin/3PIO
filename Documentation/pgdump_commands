on server


//pg_restore -c -C -d pppio_dev pgdump08102017
dropdb pppio_dev
createdb pppio_dev
pg_restore -d pppio_dev pgdump08102017

sudo rm -r ~/pppio_dev
sudo rm -r /var/www/pppio_dev

sudo cp -r ~/pppio_dev /var/www/pppio_dev


